{
  "name": "kevlar-tabs",
  "version": "1.3.0",
  "description": "Yet another better tabs library for React",
  "scripts": {
    "ladle": "ladle serve",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build:clean": "pnpm dlx rimraf dist &&  && pnpm run build",
    "build:dts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:js": "tsc -p tsconfig.build.json && vite build",
    "build": "pnpm run build:js && pnpm run build:dts",
    "test": "vitest",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "component",
    "tabs",
    "kevlar",
    "frontend"
  ],
  "license": "MIT",
  "repository": "neolitec/kevlar-tabs",
  "main": "./dist/kevlar-tabs.umd.js",
  "module": "./dist/kevlar-tabs.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kevlar-tabs.es.js",
      "require": "./dist/kevlar-tabs.umd.js"
    }
  },
  "packageManager": "pnpm@7.14.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "18.12.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@ladle/react": "^2.4.5",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.0",
    "@vitest/ui": "^0.28.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-dom": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "happy-dom": "^8.0.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.0.0",
    "rollup": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.56.1",
    "styled-components": "^5.3.6",
    "testing-library-selector": "^0.2.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.28.0",
    "vitest-dom": "^0.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
